<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="CreditCardDBJPA">
		<jta-data-source>jdbc/ccdb</jta-data-source>
		<class>com.ibm.websphere.svt.gs.ccdb.entities.CcAuthInfo</class>
		<class>com.ibm.websphere.svt.gs.ccdb.entities.CcHistory</class>
		<class>com.ibm.websphere.svt.gs.ccdb.entities.CcHistoryPK</class>
		<class>com.ibm.websphere.svt.gs.ccdb.entities.CcInfo</class>
		<class>com.ibm.websphere.svt.gs.ccdb.entities.CcOwnerInfo</class>
		<class>com.ibm.websphere.svt.gs.ccdb.entities.CcUniqueId</class>
		<properties>
			<property name="eclipselink.logging.parameters" value="true"/>
		</properties>
	</persistence-unit>
</persistence>
