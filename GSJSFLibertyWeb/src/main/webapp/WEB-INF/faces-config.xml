<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-facesconfig_3_0.xsd"
	version="3.0">
	<lifecycle>
		<phase-listener>org.j4fry.dojo.listener.SetContentTypeListener</phase-listener>
	</lifecycle>
	<application>
		<!-- <el-resolver>com.ibm.websphere.svt.gs.gsjsfweb.CustomScopeELResolver</el-resolver> -->
		<resource-library-contracts>
			<contract-mapping>
				<url-pattern>*</url-pattern>
				<url-pattern>/facelets/*</url-pattern>
				<url-pattern>/productReviewFlow/*</url-pattern>
				<contracts>common</contracts>
			</contract-mapping>
			<contract-mapping>
				<url-pattern>/facelets/*</url-pattern>
				<contracts>noDojo</contracts>
			</contract-mapping>
			<contract-mapping>
				<url-pattern>/dashboard/*</url-pattern>
				<contracts>dashboard</contracts>
			</contract-mapping>
			<!-- <contract-mapping> <url-pattern>*</url-pattern> <contracts>common</contracts> 
				</contract-mapping> -->
		</resource-library-contracts>
	</application>
	<navigation-rule>
		<from-view-id>/index.html
		</from-view-id>
		<navigation-case>
			<from-outcome>redirect</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/index.xhtml
		</from-view-id>
		<navigation-case>
			<from-action>#{pc_Index.login}</from-action>
			<from-outcome>login</from-outcome>
			<to-view-id>/facelets/welcomeUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{pc_Index.login}</from-action>
			<from-outcome>failed</from-outcome>
			<to-view-id>/facelets/loginError.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/facelets/welcomeUser.xhtml
		</from-view-id>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.detailSetup}</from-action>
			<from-outcome>category_detail</from-outcome>
			<to-view-id>/facelets/listMfgCategories.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.checkOut}</from-action>
			<from-outcome>checkOutSucceed</from-outcome>
			<to-view-id>/facelets/checkOutResult.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.checkOut}</from-action>
			<from-outcome>checkOutFailed</from-outcome>
			<to-view-id>/error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/facelets/listMfgCategories.xhtml
		</from-view-id>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.goToInventory}</from-action>
			<from-outcome>inventory_detail</from-outcome>
			<to-view-id>/facelets/showInventory.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.detailSetup}</from-action>
			<from-outcome>category_detail</from-outcome>
			<to-view-id>/facelets/listMfgCategories.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.checkOut}</from-action>
			<from-outcome>checkOutSucceed</from-outcome>
			<to-view-id>/facelets/checkOutResult.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.checkOut}</from-action>
			<from-outcome>checkOutFailed</from-outcome>
			<to-view-id>/error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/facelets/showInventory.xhtml
		</from-view-id>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.goToItem}</from-action>
			<from-outcome>item_detail</from-outcome>
			<to-view-id>/facelets/showItem.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.detailSetup}</from-action>
			<from-outcome>category_detail</from-outcome>
			<to-view-id>/facelets/listMfgCategories.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.checkOut}</from-action>
			<from-outcome>checkOutSucceed</from-outcome>
			<to-view-id>/facelets/checkOutResult.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.checkOut}</from-action>
			<from-outcome>checkOutFailed</from-outcome>
			<to-view-id>/error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/facelets/customerInfoCC.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.detailSetup}</from-action>
			<from-outcome>category_detail</from-outcome>
			<to-view-id>/facelets/listMfgCategories.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/facelets/showItem.xhtml
		</from-view-id>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.addToCart}</from-action>
			<from-outcome>addToCart</from-outcome>
			<to-view-id>/facelets/showShoppingCart.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.productReview}</from-action>
			<from-outcome>reviewProduct</from-outcome>
			<to-view-id>/facelets/reviewProduct.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>notadded</from-outcome>
			<to-view-id>/error.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.detailSetup}</from-action>
			<from-outcome>category_detail</from-outcome>
			<to-view-id>/facelets/listMfgCategories.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.checkOut}</from-action>
			<from-outcome>checkOutSucceed</from-outcome>
			<to-view-id>/facelets/checkOutResult.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.checkOut}</from-action>
			<from-outcome>checkOutFailed</from-outcome>
			<to-view-id>/error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/facelets/showShoppingCart.xhtml
		</from-view-id>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.checkOut}</from-action>
			<from-outcome>checkOutSucceed</from-outcome>
			<to-view-id>/facelets/showShoppingCart.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.checkOut}</from-action>
			<from-outcome>checkOutFailed</from-outcome>
			<to-view-id>/error.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.detailSetup}</from-action>
			<from-outcome>category_detail</from-outcome>
			<to-view-id>/facelets/listMfgCategories.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.updateCart}</from-action>
			<from-outcome>updateSuccess</from-outcome>
			<to-view-id>/facelets/showShoppingCart.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.updateCart}</from-action>
			<from-outcome>updateFailed</from-outcome>
			<to-view-id>/error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/facelets/checkOutResult.xhtml
		</from-view-id>
		<navigation-case>
			<from-outcome>category_detail
			</from-outcome>
			<to-view-id>
				/facelets/listMfgCategories.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{logout.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/logout.html</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/facelets/saveReviewSuccess.xhtml
		</from-view-id>
		<navigation-case>
			<from-outcome>category_detail
			</from-outcome>
			<to-view-id>
				/facelets/listMfgCategories.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{logout.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/logout.html</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/error.xhtml
		</from-view-id>
		<navigation-case>
			<from-outcome>category_detail
			</from-outcome>
			<to-view-id>
				/facelets/listMfgCategories.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-action>#{logout.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/logout.html</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>foundNull</from-outcome>
			<to-view-id>/error.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{customerInfo.gotoCustomerInfo}</from-action>
			<from-outcome>gotocustomer</from-outcome>
			<to-view-id>/facelets/customerInfo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{ee8Info.gotoee8Info}</from-action>
			<from-outcome>gotoee8</from-outcome>
			<to-view-id>/facelets/ee8Info.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{ee8_bv20Info.gotoee8_bv20Info}</from-action>
			<from-outcome>gotoee8_bv20</from-outcome>
			<to-view-id>/facelets/ee8_bv20Info.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{ee8_cdi20Info.gotoee8_cdi20Info}</from-action>
			<from-outcome>gotoee8_cdi20</from-outcome>
			<to-view-id>/facelets/ee8_cdi20Info.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{ee8_jsf23Info.gotoee8_jsf23Info}</from-action>
			<from-outcome>gotoee8_jsf23</from-outcome>
			<to-view-id>/facelets/ee8_jsf23Info.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{ee8_servlet40Info.gotoee8_servlet40Info}</from-action>
			<from-outcome>gotoee8_servlet40</from-outcome>
			<to-view-id>/facelets/ee8_servlet40Info.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/error.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/facelets/customerInfo.xhtml
		</from-view-id>
		<navigation-case>
			<from-action>#{garageSaleJSFActions.detailSetup}</from-action>
			<from-outcome>category_detail</from-outcome>
			<to-view-id>/facelets/listMfgCategories.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{customerInfo.changeCustomerInfo}</from-action>
			<from-outcome>changeCustomerInfo</from-outcome>
			<to-view-id>/facelets/customerInfo.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/logout.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/facelets/welcomeUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{pc_Logout.login}</from-action>
			<from-outcome>failed</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<factory>
		<exception-handler-factory>com.ibm.websphere.svt.gs.gsjsfweb.utils.GSJSFExceptionHandlerFactory</exception-handler-factory>
	</factory>
</faces-config>
