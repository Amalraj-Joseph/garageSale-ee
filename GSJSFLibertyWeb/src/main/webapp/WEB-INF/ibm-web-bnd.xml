<?xml version="1.0" encoding="UTF-8"?>
<web-bnd 
	xmlns="http://websphere.ibm.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-web-bnd_1_0.xsd"
	version="1.0">

	<virtual-host name="default_host" />

	<resource-ref name="jdbc/gsdb" binding-name="jdbc/gsdb">
		<authentication-alias name="GSDataSourceAuthData" />
	</resource-ref>
	<resource-ref name="jdbc/ccdb" binding-name="jdbc/ccdb">
		<authentication-alias name="GSDataSourceAuthData" />
	</resource-ref>
	<resource-ref name="jdbc/wsbankdb" binding-name="jdbc/wsbankdb">
		<authentication-alias name="GSDataSourceAuthData" />
	</resource-ref>
</web-bnd>
